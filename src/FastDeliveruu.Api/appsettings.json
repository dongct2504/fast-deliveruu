{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true
        }
      }
    ]
  },
  "AllowedHosts": "*",
  //"Urls": "http://*:5233",
  "ConnectionStrings": {
    "FastdeliveruuSqlConnection": "Server=fastdeliveruu.db;Database=fastdeliveruu;TrustServerCertificate=True"
  },
  "JwtSettings": {
    "Secret": "",
    "ExpiryMinutes": 60,
    "EmailConfirmationExpiryMinutes": 10,
    "Issuer": "FastDeliveruu",
    "Audience": "FastDeliveruu"
  },
  "EmailOptions": {
    "SenderEmail": "",
    "Password": ""
  },
  "Payment": {
    "VnPay": {
      "Url": "https://sandbox.vnpayment.vn/paymentv2/vpcpay.html",
      "Api": "https://sandbox.vnpayment.vn/merchant_webapi/api/transaction",
      "TmnCode": "",
      "HashSecret": "",
      "Version": "2.1.0",
      "Command": "pay",
      "CurrCode": "VND",
      "Locale": "vn",
      "ReturnUrl": "http://localhost:5233/api/v1/orders/vnpay-return"
    }
  }
}